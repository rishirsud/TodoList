    "jest": "^29.3.1",
    "jest-expo": "~50.0.2",
    "json-server": "^1.0.0-alpha.23",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.12.0",
